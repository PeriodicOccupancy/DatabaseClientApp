<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CustomersController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Customers">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TableView fx:id="customerTable" layoutX="14.0" layoutY="49.0" prefHeight="200.0" prefWidth="560.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="39.0" text="ID" />
          <TableColumn fx:id="name" prefWidth="111.0" text="Name" />
            <TableColumn fx:id="divisionName" prefWidth="109.0" text="Division" />
            <TableColumn fx:id="address" prefWidth="93.0" text="Address" />
            <TableColumn fx:id="postalCode" prefWidth="72.0" text="Postal Code" />
            <TableColumn fx:id="phone" prefWidth="68.0" text="Phone" />
            <TableColumn fx:id="divisionID" minWidth="2.0" prefWidth="67.0" text="Division ID" />
        </columns>
      </TableView>
      <Button layoutX="534.0" layoutY="361.0" mnemonicParsing="false" onAction="#returnBtn" text="Return" />
      <Button layoutX="14.0" layoutY="263.0" mnemonicParsing="false" onAction="#onActionCreate" text="Create" />
      <Button layoutX="87.0" layoutY="263.0" mnemonicParsing="false" onAction="#onActionUpdate" text="Update" />
      <Button layoutX="522.0" layoutY="263.0" mnemonicParsing="false" onAction="#onActionDelete" text="Delete" />
   </children>
</AnchorPane>
